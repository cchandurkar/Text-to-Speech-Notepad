<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.selfnotepad"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".SelfNotePadActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".ShowFileActivity"
            android:label="Create a new file" >
            <intent-filter>
                <action android:name="com.test.selfnotepad.ShowFileActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".ListFilesActivity"
            android:label="Opne File" >
            <intent-filter>
                <action android:name="com.test.selfnotepad.ListFilesActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".Text2SpeechActivity"
            android:label="Text 2 Speech Conversion" >
            <intent-filter>
                <action android:name="com.test.selfnotepad.Text2SpeechActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        
        <activity android:name="com."></activity>
    </application>

</manifest>